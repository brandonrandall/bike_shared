<form action="/trips/<%= @trip.id %>" method="post">
  <input type="hidden" name="_method" value="PUT">
  <p><input type='text' name='trip[duration]' value='<%= @trip.duration %>'/></p>
  <p><input type='text' name='trip[start_date]' value='<%= @trip.start_date %>'/></p>
  <p><input type='text' name='trip[start_station]' value='<%= @trip.start_station %>'/></p>
  <p><input type='text' name='trip[end_date]' value='<%= @trip.end_date %>'/></p>
  <p><input type='text' name='trip[bike_id]' value='<%= @trip.bike_id %>'/></p>
  <p><input type='text' name='trip[subscription_type]' value='<%= @trip.subscription_type %>'/></p>
  <p><input type='text' name='trip[zip_code]' value='<%= @trip.zip_code %>'/></p>
  <input type='submit' value='Update Trip'>
</form>
